source ~/.config/nvim/plugins.vim
set number
syntax on
set background=dark
set completeopt+=menuone
set completeopt+=noselect
set tabstop=4
set softtabstop=4
set expandtab
set autoindent
set cursorline
set showmatch
set lazyredraw
let g:airline_theme = 'gruvbox_material'
colorscheme gruvbox-material
noremap j gj
noremap k gk
